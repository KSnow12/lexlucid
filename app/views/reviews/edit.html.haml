.page-header
  %h1 Editing review

= form_for @review, html: { class: "form-horizontal", role: "form" } do |f|
  = render partial: 'form', locals: {f: f}

  .form-group
    .col-sm-offset-2.col-sm-10
      = f.submit "Add Review", class: 'btn btn-primary'
      = link_to "Cancel", user_path(@review.user_id)
